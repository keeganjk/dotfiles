paths: 
    default: artist/$albumartist/$original_year~$album%aunique{}/$track~$title
    singleton: artist/$artist/$title
    albumtype:soundtrack: st/$original_year~$album/$track~$title
    comp: comp/$original_year~$album%aunique{}/$track~$title

replace:
    '[\\/]': _
    '^\.': _
    '[\x00-\x1f]': _
    '[<>:"\?\*\|]': _
    '\.$': _
    '\s+$': ''
    '^\s+': ''
    '\s': '-'
